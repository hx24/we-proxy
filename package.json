{
  "name": "electron-anyproxy",
  "version": "0.2.1",
  "description": "A Electron client for Anyproxy",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron --inspect=5858 .",
    "pack": "electron-packager . we-proxy --out=pack --overwrite --ignore=client/node_modules --icon=icon.icns",
    "build": "electron-builder"
  },
  "build": {
    "productName": "we-proxy",
    "appId": "com.we.proxy",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "files": [
      "!**/client/*",
      "!.git",
      "!README.*",
      {
        "from": "client/dist",
        "to": "client"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hx24/we-proxy"
  },
  "keywords": [
    "Electron",
    "Anyproxy",
    "proxy",
    "electron-vue"
  ],
  "author": "fwon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fwon/electron-anyproxy/issues"
  },
  "homepage": "https://github.com/fwon/electron-anyproxy/blob/master/README.md",
  "dependencies": {
    "async": "~0.9.0",
    "async-task-mgr": ">=1.1.0",
    "body-parser": "^1.13.1",
    "co": "^4.6.0",
    "colorful": "^2.1.0",
    "commander": "~2.3.0",
    "compression": "^1.4.4",
    "cross-os-proxy": "2.0.0",
    "express": "^4.8.5",
    "iconv-lite": "^0.4.6",
    "ip": "^0.3.2",
    "juicer": "^0.6.6-stable",
    "mime-types": "2.1.11",
    "mockjs": "^1.0.1-beta3",
    "nedb": "^0.11.0",
    "node-easy-cert": "^1.0.0",
    "node-forge": "^0.6.39",
    "npm": "^2.7.0",
    "pug": "^2.0.0-beta6",
    "stream-throttle": "^0.1.3",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "electron": "^13.1.7",
    "electron-builder": "22.11.7",
    "electron-packager": "^15.3.0",
    "electron-reloader": "^1.2.1"
  }
}
